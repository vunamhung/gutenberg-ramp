{
	"name": "vunamhung/gutenberg-ramp",
	"description": "Gutenberg Ramp is a plugin that manages the state of Gutenberg in the post-edit context.",
	"keywords": [
		"wordpress",
		"gutenberg"
	],
	"homepage": "https://github.com/vunamhung/gutenberg-ramp",
	"license": "GPL-3.0+",
	"support": {
		"issues": "https://github.com/vunamhung/gutenberg-ramp/issues",
		"source": "https://github.com/vunamhung/gutenberg-ramp"
	},
	"authors": [
		{
			"name": "Vu Nam Hung",
			"email": "vunamhung@gmail.com",
			"homepage": "https://github.com/vunamhung/",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.6.0"
	},
	"autoload": {
		"files": [
			"gutenberg-ramp.php"
		]
	},
	"archive": {
		"exclude": [
			"/bin",
			"/vendor",
			"/tests",
			"/scss",
			"/sass",
			"*/composer.*",
			"*/.*",
			"*.dist",
			"*.md",
			"*.scss",
			"LICENSE",
			"*.sh"
		]
	}
}
